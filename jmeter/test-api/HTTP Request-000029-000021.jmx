          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.commons.codec.binary.Base64
import groovy.json.JsonSlurper

String token = vars.get(&quot;token&quot;)  // B·∫°n ƒë√£ c√≥ bi·∫øn n√†y r·ªìi

if (token != null &amp;&amp; token.contains(&quot;.&quot;)) {
    String payload = token.split(&quot;\\.&quot;)[1]

    // Decode Base64 (URL safe)
    payload = payload.replace(&apos;-&apos;, &apos;+&apos;).replace(&apos;_&apos;, &apos;/&apos;)
    int padding = (4 - payload.length() % 4) % 4
    payload += &quot;=&quot; * padding

    byte[] decoded = Base64.decodeBase64(payload)
    String jsonStr = new String(decoded, &quot;UTF-8&quot;)

    def json = new JsonSlurper().parseText(jsonStr)

    // G√°n c√°c gi√° tr·ªã c·∫ßn d√πng v√†o bi·∫øn JMeter
    vars.put(&quot;jwt_sub&quot;, json.sub)
    vars.put(&quot;jwt_name&quot;, json.name)
    vars.put(&quot;jwt_age&quot;, json.age?.toString())
    vars.put(&quot;jwt_job&quot;, json.job)
    vars.put(&quot;jwt_iat&quot;, json.iat)
    vars.put(&quot;jwt_admin&quot;, json.admin?.toString())

    log.info(&quot;üî• Token decoded! name = &quot; + json.name)
} else {
    log.warn(&quot;‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y token ho·∫∑c token sai format.&quot;)
}
</stringProp>
